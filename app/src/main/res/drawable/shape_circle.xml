<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <corners android:radius="60dp" />
    <gradient
        android:centerColor="#48319D"
        android:endColor="#30235B"
        android:gradientRadius="60dp"
        android:startColor="#025767"
        android:type="linear" />
    <size
        android:width="60dp"
        android:height="60dp" />
    <solid android:color="@color/material_dynamic_tertiary50" />
    <stroke
        android:width="1dp"
        android:color="#ACAAB0"
        android:dashWidth="2dp"
        android:dashGap="0dp" />
</shape>